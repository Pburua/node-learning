<!DOCTYPE html>
<html lang="en">
  <%- include('./includes/head.ejs') %>
  <body>
    <h1>You are on users page</h1>
    <% if (users.length > 0) { %>
    <ul>
      <% for (let user of users) { %>
      <li><%= user.name %></li>
      <% } %>
    </ul>
    <% } else { %>
    <p>There are no users!</p>
    <% } %>
  </body>
</html>
